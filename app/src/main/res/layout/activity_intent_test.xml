<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.wingwing.first.IntentTest">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Launch other activity"
        android:id="@+id/launchButton"
        android:onClick="launch"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call the china mobile"
        android:id="@+id/callPhoneButton"
        android:onClick="callPhone"
        android:layout_below="@+id/launchButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call the china mobile"
        android:id="@+id/sendMSG"
        android:onClick="sendTo"
        android:layout_below="@+id/callPhoneButton"
        android:layout_alignLeft="@+id/callPhoneButton"
        android:layout_alignStart="@+id/callPhoneButton" />
</RelativeLayout>
